#include <iostream>
#include <string>

using namespace std;

int main() {
    int height;

    cout << "Enter the height: " << endl;
    cin >> height;

    if (height < 1) {
        cout << "Invalid input." << endl;
        return 0;
    }

    int current_character = 0;

    for (int i = 1; i <= height; i++) {
        for (int j = 1; j <= 2*i - 1; j++) {
            cout << (char)('a' + current_character);
            current_character = (current_character + 1) % 26;
        }

        cout << endl;
    }

    return 0;
}
